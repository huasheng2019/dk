page{height:100%;background: #ffffff;}
.index{height:100%;}
.index .swiper{height: 100%;}
.index .container-one .banner{width: 100%;height: 319rpx;}
.index .container-one .list{padding: 0 30rpx;}
.index .container-one .list .item{display:flex;align-items:center;padding: 30rpx 0;border-bottom: 1rpx solid #D6D6D6;}
.index .container-one .list .item .icon{width: 98rpx;height: 98rpx;margin-right:30rpx;}
.index .container-one .list .item .des{flex: 1;overflow: hidden;}
.index .container-one .list .item .des .title,
.index .container-one .list .item .des .content{overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.index .container-one .list .item .des .title{margin-bottom:10rpx;font-size: 15px;font-weight: bold;}
.index .container-one .list .item .des .content{font-size: 12px;color: #666666;}

.index .container-two{text-align: center;font-size: 16px;color: #56B6BE;}
.index .container-two .title{padding: 45rpx 0;text-align: center;font-weight: bold;color: #37B48A;}
.index .container-two .achievement-img{display:block;width: 100%;height: 990rpx;margin: 0 auto;}
.index .container-two .bottom-tips-wrap{position: absolute;left:0;bottom:0;width: 100%;height: 300rpx;}
.index .container-two .bottom-tips-wrap .tips-bg{position: absolute;left:0;bottom:0;width: 100%;height: 300rpx;}
.index .container-two .bottom-tips-wrap .tips-wrap{position: absolute;width: 100%;padding-top:145rpx;text-align: center;font-size: 16px;line-height: 50rpx;}
.index .container-two .bottom-tips-wrap .tips-wrap .tips:nth-child(1){color: #FFFCA3;}
.index .container-two .bottom-tips-wrap .tips-wrap .tips:nth-child(2){color: #ffffff;}

.index .container-three .nodata-img{display:block;width: 279rpx;height: 280rpx;margin: 106rpx auto 43rpx;}
.index .container-three .content{padding:0 162rpx;margin-bottom:200rpx;text-align: center;line-height:48rpx;font-size: 15px;color: #666666;}
.index .container-three .wrap-btn{display:flex;justify-content:center;padding: 0 100rpx;}
.index .container-three .wrap-btn .btn{width: 100%;height: 88rpx;line-height: 88rpx;text-align: center;font-size: 16px;color: #ffffff;background: #37B48A;border-radius: 44rpx;}



